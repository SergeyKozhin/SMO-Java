<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER_LEFT" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Alpha">
         <HBox.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="alphaTextField">
         <HBox.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </HBox.margin>
      </TextField>
      <Label text="Beta">
         <HBox.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="betaTextField">
         <HBox.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </HBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#onDelete" text="-" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
         <HBox.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </HBox.margin>
      </Button>
   </children>
</fx:root>
